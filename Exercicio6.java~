class Exercicio6{
	public static void main(String[] args){
		int primeiro = 0;
		int segundo = 1;
		int valor = 0;
		System.out.println(primeiro);
		System.out.println(segundo);
		do{
			valor = primeiro + segundo;
			primeiro = segundo;
			segundo = valor;
			System.out.println(valor);
		}while(valor <= 100);
	}
}
